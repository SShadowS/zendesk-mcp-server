{
      "name": "@sshadows/zendesk-mcp-server",
      "version": "1.0.1-beta.6",
      "type": "module",
      "description": "MCP Server for Zendesk API with enhanced features",
      "main": "dist/index.js",
      "types": "dist/index.d.ts",
      "bin": {
            "zendesk-mcp": "dist/index.js"
      },
      "files": [
            "dist/**/*",
            "README.md",
            "LICENSE",
            "CLAUDE.md"
      ],
      "exports": {
            ".": {
                  "types": "./dist/index.d.ts",
                  "import": "./dist/index.js"
            }
      },
      "author": "SShadowS",
      "repository": {
            "type": "git",
            "url": "git+https://github.com/SShadowS/zendesk-mcp-server.git"
      },
      "license": "MIT",
      "keywords": [
            "zendesk",
            "mcp",
            "model-context-protocol",
            "api",
            "support",
            "tickets",
            "customer-service",
            "anthropic",
            "typescript"
      ],
      "homepage": "https://github.com/SShadowS/zendesk-mcp-server#readme",
      "bugs": {
            "url": "https://github.com/SShadowS/zendesk-mcp-server/issues"
      },
      "engines": {
            "node": ">=14.0.0"
      },
      "scripts": {
            "start": "node dist/index.js",
            "dev": "tsc --watch",
            "build": "tsc",
            "build:clean": "rm -rf dist && tsc",
            "typecheck": "tsc --noEmit",
            "start:dev": "node --watch dist/index.js",
            "inspect": "npx -y @modelcontextprotocol/inspector node dist/index.js",
            "inspect:src": "npx -y @modelcontextprotocol/inspector node src/index.js",
            "test": "echo \"No tests configured yet. Run 'npm run inspect' to test manually.\"",
            "prepublishOnly": "npm run build:clean",
            "publish:npm": "npm run build:clean && npm run test && npm publish",
            "publish:dry": "npm run build:clean && npm run test && npm publish --dry-run",
            "publish:beta": "npm run build:clean && npm run test && npm publish --tag beta",
            "publish:alpha": "npm run build:clean && npm run test && npm publish --tag alpha",
            "version:beta": "npm version prerelease --preid=beta",
            "version:alpha": "npm version prerelease --preid=alpha"
      },
      "dependencies": {
            "@anthropic-ai/sdk": "^0.52.0",
            "@modelcontextprotocol/sdk": "^1.0.0",
            "axios": "^1.6.2",
            "dotenv": "^16.3.1",
            "zod": "^3.22.4"
      },
      "devDependencies": {
            "@types/axios": "^0.9.36",
            "@types/node": "^24.0.10",
            "typescript": "^5.8.3"
      }
}
